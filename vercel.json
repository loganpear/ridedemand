{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/users/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/availability/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/reservations/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/payments/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/users/(.*)",
      "destination": "/api/users/index.py"
    },
    {
      "source": "/api/availability/(.*)",
      "destination": "/api/availability/index.py"
    },
    {
      "source": "/api/reservations/(.*)",
      "destination": "/api/reservations/index.py"
    },
    {
      "source": "/api/payments/(.*)",
      "destination": "/api/payments/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}